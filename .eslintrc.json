{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "cypress/globals": true
  },

  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:cypress/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "cypress"],
  "rules": { "cypress/no-unnecessary-waiting": "off", "no-unused-vars": "off" }
}
